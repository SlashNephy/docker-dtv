{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>SlashNephy/renovate-config"
  ],
  "packageRules": [
    {
      "minimumReleaseAge": null,
      "matchPackageNames": [
        "ghcr.io/slashnephy{/,}**"
      ]
    },
    {
      "matchFileNames": [
        "src/chinachu/Dockerfile"
      ],
      "matchPackageNames": [
        "node"
      ],
      "allowedVersions": "16.x"
    }
  ]
}
